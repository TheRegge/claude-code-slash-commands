{
  "project_type": "custom",
  "version_files": [
    {
      "path": "README.md",
      "pattern": "Version:\\s*([0-9.]+(?:-[a-zA-Z0-9.]+)?)",
      "replacement": "Version: $VERSION"
    }
  ],
  "git_tag_prefix": "v",
  "auto_commit": false,
  "commit_message_template": "chore: bump version to $VERSION"
}